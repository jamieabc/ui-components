{
  "name": "ui-components",
  "version": "1.0.1",
  "private": true,
  "description": "A collection of react components used by several internal projects.",
  "main": "lib/index.js",
  "scripts": {
    "compile": "babel --ignore __tests__,__mocks__ --copy-files --out-dir lib src",
    "watch": "babel --ignore __tests__,__mocks__ --watch --copy-files --out-dir lib src",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0",
      "react"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "dependencies": {
    "iframe-resizer": "^3.5.14",
    "lodash": "^4.17.4",
    "nprogress": "^0.2.0",
    "prop-types": "^15.5.8",
    "react-addons-create-fragment": "^15.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.23.0",
    "enzyme": "^2.8.2",
    "eslint": "^3.3.1",
    "eslint-plugin-flowtype": "^2.11.4",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.1.0",
    "eslint-plugin-react": "^6.1.2",
    "i18next": "^7.2.3",
    "jest": "^19.0.2",
    "jquery": "^3.2.1",
    "moment": "^2.10.6",
    "moment-timezone": "^0.4.1",
    "react": "^15.3.0",
    "react-addons-test-utils": "^15.5.1",
    "react-bootstrap": "^0.30.3",
    "react-datagrid": "^2.1.1",
    "react-dom": "^15.5.4",
    "react-router": "3.0.2",
    "react-test-renderer": "^15.5.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vpon/ui-components.git"
  },
  "keywords": [
    "react"
  ],
  "author": "Frontend",
  "license": "Apache License Version 2.0",
  "bugs": {
    "url": "https://github.com/vpon/ui-components/issues"
  },
  "homepage": "https://github.com/vpon/ui-components#readme",
  "jest": {
    "setupFiles": [
      "<rootDir>/setupTest.js"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "(.test)\\.(ts|tsx|js)$",
      "/lib/"
    ]
  }
}
